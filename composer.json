{
  "name": "agrism/yii2-extensions",
  "description": "Yii2 extensions installer, handler and library",
  "keywords": [
    "yii2"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/Deele/yii2-extensions/issues",
    "wiki": "https://github.com/Deele/yii2-extensions/wiki/",
    "source": "https://github.com/Deele/yii2-extensions"
  },
  "authors": [
    {
      "name": "Agris M",
      "email": "7924@inbox.lv"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.3.0",
    "ext-mbstring": "*",
    "yiisoft/yii2": "~2.0.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.6.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "yiisoft/yii2-coding-standards": "^2.0.4"
  },
  "autoload": {
    "psr-4": {
      "deele\\extensions\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "scripts": {
    "sniff-code": "./vendor/bin/phpcs --extensions=php --standard=Yii2 ./src"
  }
}
